  "solution": {
    "outline": [],
    "steps": [
      {
        "idx": 1,
        "description": "设常数 C=∫ f(t)cos t dt，则 f(x)=sin x+3C",
        "math": "C=\\int_0^{\\pi/2} f(t)\\cos t\\,dt"
      },
      {
        "idx": 2,
        "description": "两边乘 cos x 积分求 C",
        "math": "C=\\int_0^{\\pi/2}\\sin x\\cos x\\,dx+3C\\int_0^{\\pi/2}\\cos x\\,dx"
      },
      {
        "idx": 3,
        "description": "解得 C=-1/4，代回并求所需积分",
        "math": "f(x)=\\sin x-\\tfrac34,\\ \\int_0^{\\pi/2}f(x)dx=1-\\tfrac{3\\pi}{8}"
      }
    ],
