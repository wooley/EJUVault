    "steps": [
      {
        "idx": 1,
        "description": "導関数を因数分解して臨界点を確認",
        "math": "f'(x)=x^2-(t+2)x+2t=(x-2)(x-t)"
      },
      {
        "idx": 2,
        "description": "境界値f(4)を計算",
        "math": "f(4)=6"
      },
      {
        "idx": 3,
        "description": "t>2では極大点x=2の値が6を超える条件",
        "math": "f(2)=2t-\\frac23>6 \\Rightarrow t>\\frac{10}{3}"
      },
      {
        "idx": 4,
        "description": "t<2では極大点x=tの値が6を超える条件（因数分解を利用）",
        "math": "f(t)-6=-\\frac16(t+2)(t-4)^2>0 \\Rightarrow t<-2"
      }
    ],
